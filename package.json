{
  "name": "macoolka-type-ast",
  "bin": {
    "md": "./lib/cli/md",
    "demo": "./lib/cli/demo"
  },
  "version": "0.2.0",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "types": "lib/index.d.ts",
  "author": "fastspeeed@gmail.com",
  "scripts": {
    "reinstall": "rm -rf node_modules/ && yarn install",
    "clean": "rm -rf lib/ coverage/ ",
    "lint": "tslint -p tsconfig.json",
    "lint:fix": "tslint -p tsconfig.json --fix",
    "test": "jest",
    "test:watch": "jest --config jest.config.json --watch ./src",
    "build": "rm -rf lib/ && tsc ",
    "precommit": "npm run lint",
    "prepush": "npm run test",
    "coverage": "rimraf coverage && jest --coverage",
    "docs": "macoolka-doc",
    "all": "npm run clean && tsc && jest  --coverage && tslint -p tsconfig.json"
  },
  "dependencies": {
    "doctrine": "^3.0.0",
    "fp-ts": "^2.0.2",
    "macoolka-app": "^0.2.2",
    "macoolka-compare": "^0.2.2",
    "macoolka-store-core": "^0.2.2",
    "macoolka-store-fs": "^0.2.0",
    "macoolka-type-model": "^0.2.1",
    "ts-morph": "^3.1.3"
  },
  "devDependencies": {
    "@types/doctrine": "^0.0.3",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.2",
    "@types/prettier": "^1.18.2",
    "jest": "^24.9.0",
    "macoolka-doc": "^0.2.1",
    "macoolka-prettier": "^0.2.0",
    "macoolka-typescript": "^0.2.1",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.19.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.6.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,tsx,ts}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(./src/*|\\.(test|spec))\\.(ts|tsx)$"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/macoolka/macoolka-type-ast.git"
  },
  "homepage": "https://github.com/macoolka/macoolka-type-ast",
  "keywords": [
    "macoolka",
    "typescript",
    "ast",
    "functional-programming"
  ],
  "license": "MIT"
}
